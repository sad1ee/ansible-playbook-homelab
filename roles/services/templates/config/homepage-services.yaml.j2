---
- Admin:
    - Beszel:
        icon: beszel
        href: "https://beszel.{{ services_domain_base }}"
        server: local-docker
        container: beszel
        widget:
          type: beszel
          url: "http://beszel:{{ services_webui_port.beszel }}"
          username: "{{ services_beszel_user }}"
          password: "{{ services_beszel_pass }}"
          systemId: pn326lg1axy991d
          fields: ["cpu", "memory", "disk", "network"]
          version: 2

    - Dozzle:
        icon: dozzle
        href: "https://dozzle.{{ services_domain_base }}"
        server: local-docker
        container: dozzle

    - Ntfy:
        icon: ntfy
        href: "https://ntfy.{{ services_domain_base }}"
        server: local-docker
        container: ntfy

    - Whats-up-docker:
        icon: whats-up-docker
        href: "https://wud.{{ services_domain_base }}"
        server: local-docker
        container: wud
        widget:
          type: whatsupdocker
          url: "http://wud:{{ services_webui_port.wud }}"
          # username: username # optional
          # password: password # optional

    - Pihole:
        icon: pi-hole-unbound
        href: "https://pihole.{{ services_domain_base }}"
        server: local-docker
        container: pihole
        widget:
          type: pihole
          url: "http://host.docker.internal:{{ services_webui_port.pihole }}"
          version: 6

    - Traefik:
        icon: traefik
        href: "https://traefik.{{ services_domain_base }}"
        server: local-docker
        container: traefik
        widget:
          type: traefik
          url: "http://traefik:{{ services_webui_port.traefik_webinternal }}"
          username: "{{ services_traefik_user }}"
          password: "{{ services_traefik_pass }}"

- Services:
    - Paperless:
        icon: paperless
        href: "https://paperless.{{ services_domain_base }}"
        server: local-docker
        container: paperless
        widget:
          type: paperlessngx
          url: "http://paperless:{{ services_webui_port.paperless }}"
          username: "{{ services_paperless_user }}"
          password: "{{ services_paperless_pass }}"


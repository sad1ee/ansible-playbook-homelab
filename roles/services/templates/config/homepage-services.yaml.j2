    - Syncthing:
        icon: syncthing
        href: "https://syncthing.{{ services_domain_base }}"
        server: local-docker
        container: syncthing

- Media:
    - Frontend:
        - Jellyfin:
            icon: jellyfin
            href: "https://jellyfin.{{ services_domain_base }}"
            description: Media server
            server: local-docker
            container: jellyfin
            widget:
              type: jellyfin
              url: "http://jellyfin:{{ services_webui_port.jellyfin }}"
              key: "{{ services_jellyfin_key }}"
              fields: ["movies", "series", "episodes"]
              enableBlocks: true # optional, defaults to false
              enableNowPlaying: true # optional, defaults to true
              enableUser: true # optional, defaults to false
              enableMediaControl: false # optional, defaults to true
              showEpisodeNumber: true # optional, defaults to false
              expandOneStreamToTwoRows: true # optional, defaults to true

        - Jellyseerr:
            icon: jellyseerr
            href: "https://jellyseerr.{{ services_domain_base }}"
            description: Media search
            server: local-docker
            container: jellyseerr
            widget:
              type: jellyseerr
              url: "http://jellyseerr:{{ services_webui_port.jellyseerr }}"
              fields: ["pending", "approved", "available", "issues"]
              key: "{{ services_jellyseerr_key }}"

        - CWA:
            icon: calibre-web
            href: "https://cwa.{{ services_domain_base }}"
            server: local-docker
            container: cwa
            widget:
              type: calibreweb
              url: "http://cwa:{{ services_webui_port.cwa }}"
              username: "{{ services_calibre_user }}"
              password: "{{ services_calibre_pass }}"

        - CWAD:
            icon: calibre-web-automated-book-downloader
            href: "https://cwad.{{ services_domain_base }}"
            server: local-docker
            container: cwad

    - Backend:
        - Radarr:
            icon: radarr
            href: "https://radarr.{{ services_domain_base }}"
            server: local-docker
            container: radarr
            widget:
              type: radarr
              url: "http://radarr:{{ services_webui_port.radarr }}"
              key: "{{ services_radarr_key }}"
              enableQueue: true # optional, defaults to false

        - Sonarr:
            icon: sonarr
            href: "https://sonarr.{{ services_domain_base }}"
            server: local-docker
            container: sonarr
            widget:
              type: sonarr
              url: "http://sonarr:{{ services_webui_port.sonarr }}"
              key: "{{ services_sonarr_key }}"
              enableQueue: true # optional, defaults to false

        - Prowlarr:
            icon: prowlarr
            href: "https://prowlarr.{{ services_domain_base }}"
            server: local-docker
            container: prowlarr
            widget:
              type: prowlarr
              url: "http://prowlarr:{{ services_webui_port.prowlarr }}"
              key: "{{ services_prowlarr_key }}"

        - Bazarr:
            icon: bazarr
            href: "https://bazarr.{{ services_domain_base }}"
            server: local-docker
            container: bazarr
            widget:
              type: bazarr
              url: "http://bazarr:{{ services_webui_port.bazarr }}"
              key: "{{ services_bazarr_key }}"

        - QBittorrent:
            icon: qbittorrent
            href: "https://qbittorrent.{{ services_domain_base }}"
            server: local-docker
            container: qbittorrent
            widget:
              type: qbittorrent
              url: "http://gluetun:{{ services_webui_port.qbittorrent }}"
              username: "{{ services_qbittorrent_user }}"
              password: "{{ services_qbittorrent_pass }}"
              enableLeechProgress: true # optional, defaults to false
              enableLeechSize: true # optional, defaults to false


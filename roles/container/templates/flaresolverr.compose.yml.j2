services:
  flaresolverr:
    image: ghcr.io/flaresolverr/flaresolverr:latest

    labels:
      dev.dozzle.group: Backend

    environment:
      TZ: "{{ container_tz }}"
      LOG_LEVEL: info # debug, info

    container_name: flaresolverr
    hostname: flaresolverr
    restart: unless-stopped

    networks:
      - flaresolverr

networks:
  flaresolverr:
    external: true
    name: flaresolverr
